<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>VL.Addons</id>
    <version>0.3.2</version>
    <title>VL.Addons</title>
    <authors>vvvv community</authors>
    <owners>vvvv community</owners>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <readme>README.md</readme>
    <projectUrl>https://github.com/bj-rn/VL.Addons</projectUrl>
    <repository type="git" url="https://github.com/bj-rn/VL.Addons.git" />
    <summary>Various addons for VL</summary>
    <description>Various VL addons provided by the community.</description>
    <copyright>Copyright Â© 2024</copyright>
    <tags>VL, vvvv, gamma, addons, texturefx, shader, effect</tags>
    <dependencies>
      <dependency id="CodingSeb.ExpressionEvaluator" version="1.4.39" />
    </dependencies>
    <icon>resources\icon.png</icon>
  </metadata>
  <files>
    <file src="..\*.vl"/>
    <file src="..\lib\net8.0\*.dll" target="lib\net8.0" />
    <file src="..\vl\*.vl" target="vl"/>
    <file src="..\vl\shaders\**\*.sdsl" target="vl\shaders"/>
    <file src="..\help\**\*.vl" target="help"/>
    <file src="..\resources\**\*.*" target="resources" exclude="" />
    <file src="..\README.md" />
    <file src="..\LICENSE" />
  </files>
</package>
